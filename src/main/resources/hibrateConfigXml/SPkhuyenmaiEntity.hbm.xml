<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.demo.Hibernate.SPkhuyenmaiEntity" table="SPkhuyenmai" schema="dbo" catalog="Sellphones">
        <composite-id mapped="true" class="com.example.demo.Hibernate.SPkhuyenmaiEntityPK">
            <key-property name="maSpkm">
                <column name="MaSPKM" sql-type="int"/>
            </key-property>
            <key-property name="maSp">
                <column name="MaSP" sql-type="int"/>
            </key-property>
        </composite-id>
        <property name="giamgia">
            <column name="Giamgia" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>