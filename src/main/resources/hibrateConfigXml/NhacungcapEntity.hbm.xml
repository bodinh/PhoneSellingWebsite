<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.demo.Hibernate.NhacungcapEntity" table="Nhacungcap" schema="dbo" catalog="Sellphones">
        <id name="idNhacungcap">
            <column name="IDNhacungcap" sql-type="int"/>
        </id>
        <property name="tenNhaungcap">
            <column name="TenNhaungcap" sql-type="nvarchar(100)" length="100"/>
        </property>
        <property name="phonenumber">
            <column name="phonenumber" sql-type="char(15)" length="15"/>
        </property>
        <property name="email">
            <column name="email" sql-type="nvarchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>