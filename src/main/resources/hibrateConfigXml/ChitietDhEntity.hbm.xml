<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.demo.Hibernate.ChitietDhEntity" table="ChitietDH" schema="dbo" catalog="Sellphones">
        <id name="maChitietDh">
            <column name="MaChitietDH" sql-type="int"/>
        </id>
        <property name="maDh">
            <column name="MaDH" sql-type="int" not-null="true"/>
        </property>
        <property name="maSp">
            <column name="MaSP" sql-type="int" not-null="true"/>
        </property>
        <property name="soluong">
            <column name="Soluong" sql-type="int" not-null="true"/>
        </property>
        <property name="thanhtien">
            <column name="Thanhtien" sql-type="float" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>